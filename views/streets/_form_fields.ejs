<!-- form fields for street creation-->
<div class="form-row">
	<div class="form-item">
		<label>Address Name</label>
		<input type="text" name="addressName" value="<%= street.addressName %>">
	</div>
	<div class="form-item">
		<label>Address Number</label>
		<input type="number" min="0" name="addressNum" value="<%= street.addressNum %>">
	</div>
	<div class="form-item">
		<label>Postal Code</label>
		<input type="number" name="postal" value="<%= street.postal %>">
	</div>
	<div class="form-item">
		<label>Google Maps Link</label>
		<input type="text" name="maps" value="<%= street.maps %>">
	</div>

	</div>
</div>